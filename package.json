{
    "name": "typescript-sample",
    "homepage": "https://jameslead00.github.io/redIT_Baumgartner_BildUpload/",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@azure/msal-browser": "^4.0.0",
        "@azure/msal-react": "^3.0.0",
        "@emotion/react": "^11.10.5",
        "@emotion/styled": "^11.10.5",
        "@mui/icons-material": "^5.10.16",
        "@mui/material": "^5.10.17",
        "@types/node": "^16.18.10",
        "@types/react": "^19.1.3",
        "@types/react-dom": "^19.1.3",
        "@types/react-router-dom": "^5.1.7",
        "dexie": "^4.2.1",
        "react": "^19.2.1",
        "react-dom": "^19.2.1",
        "react-router-dom": "^6.7.0",
        "typescript": "^4.9.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build && workbox generateSW workbox-config.js",
        "build:package": "cd ../../../ && npm run build:all --workspace=lib/msal-react",
        "test": "jest --watchAll=false --config jest.config.cjs",
        "test:ci": "jest --coverage --runInBand --config jest.config.cjs",
        "test:e2e": "jest",
        "test:unit": "jest --runInBand --config jest.config.cjs",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.5.14",
        "e2e-test-utils": "file:../../e2eTestUtils",
        "gh-pages": "^6.3.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.5.0",
        "jest-junit": "^16.0.0",
        "jest-puppeteer": "^11.0.0",
        "msw": "^2.12.4",
        "puppeteer": "^24.32.1",
        "react-scripts": "^5.0.1",
        "rollup": "^4.52.5",
        "shx": "^0.4.0",
        "start-server-and-test": "^2.1.3",
        "ts-jest": "^29.4.6",
        "workbox-cli": "^7.3.0",
        "workbox-webpack-plugin": "^7.3.0"
    },
    "jest-junit": {
        "suiteNameTemplate": "TypeScript Sample Tests",
        "outputDirectory": ".",
        "outputName": "test-results.xml"
    }
}
